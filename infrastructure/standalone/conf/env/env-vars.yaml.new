  # OAUTH_SCOPE: openid%20profile%20email
  # OAUTH_STATE: xyzABC123

  UI_MAIN_USER_FACING_HTTP_PATH: https://dataprofiler
  USER_FACING_UI_HTTP_PATH: http://localhost:8080
  USER_FACING_API_HTTP_PATH: http://localhost:9000
